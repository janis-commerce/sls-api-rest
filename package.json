{
  "name": "@janiscommerce/sls-api-rest",
  "version": "3.2.0",
  "description": "An integration handler for Serverless and Janis Rest APIs",
  "main": "lib/index.js",
  "scripts": {
    "test": "export TEST_ENV=true; mocha --exit -R nyan --recursive tests/",
    "test-ci": "nyc --reporter=lcov --reporter=text-summary mocha --exit --recursive tests/",
    "watch-test": "export TEST_ENV=true; mocha --exit -R nyan -w --recursive tests/",
    "coverage": "nyc npm test",
    "lint": "eslint lib/ tests/",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janis-commerce/sls-api-rest.git"
  },
  "author": "Janis",
  "license": "ISC",
  "homepage": "https://github.com/janis-commerce/sls-api-rest#readme",
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "sinon": "^12.0.1"
  },
  "files": [
    "lib/"
  ],
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "@janiscommerce/api": "^6.4.0",
    "@janiscommerce/sls-api-response": "^1.5.0",
    "amazon-api-gateway-querystring": "1.0.5"
  }
}
